{
  "name": "darttoernooi",
  "productName": "Darttoernooi",
  "version": "1.11.2",
  "description": "Speelschema voor darttoernooien (of een ander toernooi met poules).",
  "main": "src/index.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\"",
    "build-installer-windows": "electron-builder --windows nsis:x64",
    "build-linux": "electron-builder --linux"
  },
  "build": {
    "asar": false,
    "appId": "Darttoernooi",
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "src/icons/appIcon.ico"
    },
    "nsis": {
      "installerIcon": "src/icons/appIcon.ico",
      "uninstallDisplayName": "Darttoernooi",
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    }
  },
  "keywords": [],
  "author": {
    "name": "HeadlessHamsterr",
    "email": "joost_rietdijk@live.nl"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {
        "icon": "/icons/appIcon.icon"
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "darttoernooi"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  },
  "dependencies": {
    "@mdi/react": "^1.5.0",
    "broadcast-address": "^1.0.2",
    "dgram": "^1.0.1",
    "electron-squirrel-startup": "^1.0.0",
    "http-terminator": "^3.0.3",
    "ip": "^1.1.5",
    "jquery": "^3.6.0",
    "jspdf": "^2.4.0",
    "monitor.io": "^0.1.0",
    "play-sound": "^1.1.5",
    "qrcode": "^1.5.0",
    "socket.io": "^4.3.2",
    "sound-play": "^1.1.0",
    "xmlhttprequest": "^1.8.0"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.60",
    "@electron-forge/maker-deb": "^6.0.0-beta.60",
    "@electron-forge/maker-rpm": "^6.0.0-beta.60",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.60",
    "@electron-forge/maker-zip": "^6.0.0-beta.60",
    "@socket.io/sticky": "^1.0.1",
    "datagram": "^0.9.2",
    "electron": "14.0.0",
    "electron-builder": "22.10.5",
    "electron-compile": "^6.4.4",
    "electron-forge": "^5.2.4",
    "electron-json-storage": "^4.5.0",
    "fs": "^0.0.1-security",
    "http-terminator": "^3.0.3",
    "ip": "^1.1.5",
    "monitor.io": "^0.1.0",
    "qrcode": "^1.5.0",
    "socket.io": "^4.3.2"
  }
}
